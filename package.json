{
  "name": "@phoenixlan/phoenix.js",
  "version": "3.5.2",
  "description": "Phoenix LAN api javascript wrapper",
  "main": "build/index.js",
  "module": "build/index.es.js",
  "types": "build/index.d.ts",
  "author": "petterroea et al",
  "license": "GPL",
  "private": false,
  "repository": {
    "url": "https://github.com/phoenixlan/phoenixjs"
  },
  "scripts": {
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "build": "rollup --config",
    "lint": "tslint --project tsconfig.json -c tslint.commit.json",
    "autolint": "lint --fix",
    "test": "jest --runInBand --forceExit",
    "prettify": "prettier **.ts --write",
    "commit": "cz"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@types/jest": "^26.0.14",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "eslint": "^7.5.0",
    "jest": "^26.4.2",
    "pinst": "^2.1.6",
    "prettier": "^2.0.5",
    "rollup": "^2.38.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.29.0",
    "typescript": "^4.1.3",
    "tslint": "^6.1.3"
  },
  "dependencies": {
    "jwt-decode": "^3.1.2"
  }
}
